@model IEnumerable<Messenger.Models.UserMassageDTO>

@if (Model != null)
{
    @foreach (var item in Model)
    {
        <a href="@Url.Action("Chat", "Messages", new { userName = item.UserData })">
            <div class="row border p-3">
                <div class="row ">
                    <h3 class="text-dark">@item.UserData</h3>
                </div>
                <div class="row h5">
                    <h5 class="text-dark">@item.Message</h5>
                </div>
            </div>
        </a>
    }
}
else
{

}